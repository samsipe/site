                a.link(href!="<%= link %>", target="_blank") <%= company %>
                <% } else { %>
                <%= company %>
                <% } %>
            .dot
            h4.subtitle <%= position %>
            h5.location <%= location %>
            .content 
                <% for(var i in info) { %> 
                p  <%= info[i] %> 
                <% } %> 
        hr